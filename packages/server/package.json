{
  "name": "@mocksi/bilan-server",
  "version": "0.4.1",
  "description": "Self-hostable analytics server for AI trust tracking - Process events from Bilan SDK with SQLite storage and REST API",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=14.17.0"
  },
  "bin": {
    "bilan": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "bilan",
    "server",
    "analytics",
    "sqlite",
    "ai-analytics",
    "trust-analytics",
    "self-hosted",
    "fastify",
    "event-processing",
    "conversation-analytics",
    "feedback-tracking",
    "ai-monitoring",
    "typescript",
    "rest-api",
    "docker"
  ],
  "author": "Mocksi <hello@mocksi.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mocksi/bilan.git",
    "directory": "packages/server"
  },
  "homepage": "https://github.com/Mocksi/bilan#readme",
  "bugs": {
    "url": "https://github.com/Mocksi/bilan/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Mocksi"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.12",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/rate-limit": "10.3.0",
    "@mocksi/bilan-sdk": "file:../sdk",
    "better-sqlite3": "12.2.0",
    "fastify": "5.4.0"
  }
}
